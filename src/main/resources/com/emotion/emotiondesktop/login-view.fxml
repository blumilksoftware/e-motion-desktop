<AnchorPane prefHeight="768.0" prefWidth="1366.0" style="-fx-background-color: #f4f8fd;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.emotion.emotiondesktop.Controller.LoginController">
   <children>
      <VBox alignment="CENTER" fillWidth="false" layoutX="361.0" layoutY="182.0" prefHeight="450.0" prefWidth="600.0" spacing="120.0" style="-fx-background-color: #ffffff; -fx-opacity: 80%;">
         <children>
            <VBox alignment="CENTER" prefHeight="167.0" prefWidth="210.0" spacing="30.0">
               <children>
                  <VBox alignment="CENTER">
                     <children>
                        <Text fill="#527aba" strokeType="OUTSIDE" strokeWidth="0.0" text="E-mail">
                           <font>
                              <Font name="Microsoft Sans Serif" size="16.0" />
                           </font>
                        </Text>
                        <TextField fx:id="emailField" style="-fx-background-color: #ffffff; -fx-border-style: solid; -fx-border-color: #aabddd; -fx-border-radius: 5;" />
                     </children>
                  </VBox>
                  <VBox alignment="CENTER">
                     <children>
                        <Text fill="#527aba" strokeType="OUTSIDE" strokeWidth="0.0" text="Password">
                           <font>
                              <Font name="Microsoft Sans Serif" size="16.0" />
                           </font>
                        </Text>
                        <PasswordField fx:id="passwordField" style="-fx-background-color: #ffffff; -fx-border-style: solid; -fx-border-color: #aabddd; -fx-border-radius: 5;" />
                     </children>
                  </VBox>
                  <Button fx:id="loginButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#userLogIn" prefHeight="32.0" prefWidth="110.0" style="-fx-background-color: #ffffff; -fx-border-color: #4b5563; -fx-border-style: solid; -fx-border-color: #aabddd; -fx-border-radius: 20;" text="Login" textAlignment="CENTER" textFill="#527aba">
                     <font>
                        <Font name="Microsoft Sans Serif" size="16.0" />
                     </font>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
                  <Label fx:id="loginInfo" />
               </children>
            </VBox>
         </children>
      </VBox>
      <ImageView fitHeight="127.0" fitWidth="267.0" layoutX="528.0" layoutY="71.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../images/logo.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
